<body>

    <div class="topnav">
        <img src="/images/logoacad.png" style="width:100px;height:90px;">
        <a href="/">Home</a>
        <a href="/about" class="active">About</a>
        <a href="/contacts" >Contacts</a>
    </div>

      <div class="about_section layout_padding">
        <div class="container-fluid">  
          <div class="row">
            
            <div class="col-md-6">
              
              <div >
                <a class="fa fa-plus" style="font-size:30px;color:rgb(32, 128, 0)" href="/add-about">Add</a>
                <% card.forEach((row) => { %>
                <ul class="about_taital" style="width: 100%;
                background-image: url('<%= row.image %>');
                ">
                  

                        <a class="fa fa-edit" style="font-size:24px" href="/editabout/<%=row.id%>"></a>
                        
                        <li class="highest_text"><%= row.heading %></li>
                        <li class="lorem_text"><%= row.body %></li>
                    </ul>
                    <% }); %>
              </div>
            </ul>
            </div>
            <!--<div class="col-md-6">
              <div class="image_2"><img src="../images/learners1.jpeg"></div>
            </div> -->
          </div>
        </div>

        <div class="copyright_section">
            <div class="container">
              <p class="copyright">2023 All Rights Reserved. Jimcy Academy</p>
            </div>
          </div>
          <!-- copyright section end -->
          <!-- Javascript files-->
          <script src="/js/jquery.min.js"></script>
          <script src="/js/popper.min.js"></script>
          <script src="/js/bootstrap.bundle.min.js"></script>
          <script src="/js/jquery-3.0.0.min.js"></script>
          <script src="/js/plugin.js"></script>
          <!-- sidebar -->
          <script src="/js/jquery.mCustomScrollbar.concat.min.js"></script>
