<style>
  footer {
display: flex;
justify-content: space-between;
background-color: #3688f4;
color: white;
padding: 20px;
}

.footer-column {
flex-basis: 50%;
text-align: center;
}

.footer-column h3 {
margin-top: 0;
}

.footer-column a {
display: block;
color: white;
text-decoration: none;
margin-bottom: 10px;
}

.footer-column a.active {
font-weight: bold;
}

.footer-column a:hover {
text-decoration: underline;
}


/* Desktop Navbar */
@media (min-width: 768px) {
  .mobile-nav {
    display: none;
  }

  .topnav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 90px;
    padding: 0 20px;
    background-color: #3688f4;
    color: white;
  }

  .topnav a {
    color: white;
    text-decoration: none;
    margin-left: 20px;
    font-size: 18px;
  }

  .topnav a.active {
    font-weight: bold;
  }

  .topnav a:hover {
    text-decoration: underline;
  }

  .banner_section{
    width: 100%;
    height: 800px;
    float: left;
    background-size: 100%;
    background-repeat: no-repeat;

  }

  .intro-section{
    width: 100%;
    height: 100%;
    height: 800px;
    background-size: 100%;
    background-repeat: no-repeat;

  }
}

/* Mobile Navbar */
@media (max-width: 767px) {
  .topnav {
    display: none;
  }

  .mobile-nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 90px;
    padding: 0 20px;
    background-color: #3688f4;
    color: white;
  }

  .mobile-nav img {
    width: 100px;
    height: 90px;
  }

  .icon {
    font-size: 28px;
    cursor: pointer;
  }

  #myLinks {
    display: none;
    position: absolute;
    top: 90px;
    left: 0;
    right: 0;
    background-color: #333;
    z-index: 1;
  }

  #myLinks a {
    display: block;
    color: white;
    padding: 10px;
    text-align: center;
    font-size: 18px;
  }

  #myLinks a.active {
    font-weight: bold;
  }

  #myLinks a:hover {
    text-decoration: underline;
  }

  .mobile-nav.active #myLinks {
    display: block;
  }

  .banner_section{
    width: 100%;
    height: 100%;
    float: left;
    background-size: 100%;
    background-repeat: no-repeat;

  }
  .intro-section{
    height: 400px;
    background-size: cover;
  }

  
}

</style>
</head>

